generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id           Int      @id @default(autoincrement())
  student_id   String    @db.VarChar(255) @unique
  student_name  String?  @db.VarChar(255)
  course       String?  @db.VarChar(100)
  present_date  DateTime @default(now())
}